<div class="container">
    <a href="/realTimeProducts">Real time products</a>
    <h1>Home</h1>
    <h2>Productos</h2>
   <table class="table table-striped table-primary">
    <thead>
        <tr>
        <th >#id</th>
        <th >title</th>
        <th >description</th>
        <th >price</th>
        <th >thumbnail</th>
        <th >code</th>
        <th >stock</th>
        <th >status</th>
        <th >price</th>
        </tr>
    </thead>
    {{#each products}}
    <tbody>
        <tr>
        <th scope="row" >{{id}}</th>
        <th >{{title}}</th>
        <th >{{description}}</th>
        <th >{{price}}</th>
        <th ><img class="imagen" src="{{thumbnail}}" alt=""></th>
        <th >{{code}}</th>
        <th >{{stock}}</th>
        <th >{{status}}</th>
        <th >{{price}}</th>
        </tr>
        
    </tbody>
{{/each}}
</table>
</div>

</div>